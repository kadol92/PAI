### PROTECT FILES ###
<FilesMatch "\.(htaccess|htpasswd|ini|fla|psd|log|sh)$">
Order Allow,Deny
Deny from all
</FilesMatch>

RewriteEngine on
RewriteRule strona-glowna index.php
RewriteRule filmy movies.php
RewriteRule seriale series.php
RewriteRule informacje info.php
RewriteRule user userInfo.php
RewriteRule dodaj-wpis addArticle.php
RewriteRule zarzaj-wpisami articles.php
RewriteRule edytuj-wpis edit_Article.php

